// profile for wave and fusion filesystem enabled batch pipeline

workDir = 's3://openscpca-nf-data/work'
docker.enabled = true
wave.enabled = true
fusion.enabled = true


aws{
  batch{
    maxTransferAttempts = 3
    maxSpotAttempts = 2
  }
  region = 'us-east-2'
}


process{
  executor = 'awsbatch'
  scratch = false
  queue = 'openscpca-nf-batch-default-queue'
}
